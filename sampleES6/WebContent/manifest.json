{
    "sap.app": {
        "id": "influenz.de.sample",
        "type": "application",
        "i18n": "i18n/i18n.properties",
        "title": "{{APPLICATION_TEXT}}"
    },
    "sap.ui": {
        "technology": "UI5",
        "deviceTypes": {
            "desktop": true,
            "tablet": true,
            "phone": true
        },
        "supportedThemes": [
            "sap_bluecrystal",
            "sap_belize"
        ]
    },
    "sap.ui5": {
        "resourceRoots": {},
        "componentName": "influenz.de.sample",
        "rootView": "influenz.de.sample.view.App",
        "dependencies": {
            "libs": {
                "sap.m": {}
            }
        },
        "resources": {
            "js": [
                {
                    "uri": "./libraries/three.min.js"
                },
                {
                    "uri": "./libraries/objloader.min.js"
                }
            ]
        },
        "models": {
            "i18n": {
                "type": "sap.ui.model.resource.ResourceModel",
                "settings": {
                    "bundleName": "influenz.de.sample.i18n.i18n"
                }
            } 
        },
        "routing": {
            "config": {
                "routerClass": "sap.m.routing.Router",
                "viewType": "XML",
                "viewPath": "influenz.de.sample.view",
                "controlId": "appId",
                "controlAggregation": "pages"
            },
            "routes":[
                {
                    "pattern" : "",
                    "name":"defaultRoute",
                    "target":"defaultTarget"
                }
            ],
            "targets":{
                "defaultTarget":{
                    "viewName":"Main"
                }
            }
        }
    }
}